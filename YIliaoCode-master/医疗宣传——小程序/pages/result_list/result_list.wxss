.container{
  background: #F8F8F8; 
  height: 1444rpx; 
  display: block; 
  box-sizing: border-box; 
  left: 0rpx; 
  top: 0rpx
}
.text{
  background-color: white;
  display: flex;
  align-items: center;
  justify-content: center;
}
.text text{
  font-size: 35px;
  color: black;
  font-weight: normal;
}
.result-item{
  margin-top: 100rpx;
}
.result-list{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.result{
  margin-top: 50rpx;
  background-color: white;
  padding: 30rpx;
  border-radius: 30rpx;
  width: 85%;
  border: solid #38C0AA;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.result van-icon{
  margin-left: 65%;
}
.mid{
  display: flex;
  flex-direction: column;
}
.text1{
  color: #38C0AA;
  font-size: large;
  font-weight: bold;
}