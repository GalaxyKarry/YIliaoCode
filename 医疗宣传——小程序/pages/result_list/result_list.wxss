page{
  background-color: #F0F3FF;
  height: 100%;
}
.container{
  display: flex;
  flex-direction: column;
  margin-left: 30rpx;
  margin-bottom: 30rpx;
}
.title{
  font-weight: bolder;
  font-size: xx-large;
}
.subTitle{
  font-weight: bolder;
  color: #929DB3;
}

.result-list{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.result{
  margin-top: 50rpx;
  background-color: #BBB7E8;
  border-radius: 30rpx;
  width: 90%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  box-shadow: 10rpx 10rpx 20rpx rgba(0, 0, 0, 0.20);
}
.result0{
  margin-top: 50rpx;
  background-color: #DDDBFF;
  border-radius: 30rpx;
  width: 90%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  box-shadow: 10rpx 10rpx 20rpx rgba(0, 0, 0, 0.20);
}

.mid{
  display: flex;
  flex-direction: column;
  margin-left: 50rpx;
}
.text1{
  font-size: 40rpx;
  font-weight: bolder;
}