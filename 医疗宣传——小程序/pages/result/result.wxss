.container{
  height: 1700rpx;
  background: #F8F8F8; 
  display: flex; 
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.container video{
  margin-top: 50rpx;
}

.outcome{
  border: #8BC34A solid 10rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 350rpx;
  width: 350rpx;
  border-radius: 500rpx;
}
.outcome .result{
  font-size: 35px;
  color: #8BC34A;
  font-weight: bold;
}
.outcome .score{
  font-size: 25px;
  color: #8BC34A;
  font-weight: bold;
}

.info{
  display: flex;
  flex-direction: column;
  margin-top: 150rpx;
  margin-bottom: 100rpx;
}
.info .item{
  display: flex;
  flex-direction: row;
  justify-content:space-between;
  align-items: center;
  margin: 15rpx;
}
.item .title{
  font-size: 20px;
  color: black;
  font-weight: bolder;
}
.item .content{
  font-size: 18px;
  color: black;
  font-weight: normal;
}
.but{
  margin-top: 50rpx;
}